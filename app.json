{
  "expo": {
    "name": "SpaceLaunchApp",
    "slug": "SpaceLaunchApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18242F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": 1
    },
    "android": {
      "package": "de.tim0_12432.spacelaunchapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#18242F"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
